{
  "name": "run",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && cp instrument.server.mjs dist/server",
    "types:check": "tsgo --noEmit",
    "types:watch": "tsgo --noEmit --watch --preserveWatchOutput",
    "deploy": "wrangler deploy",
    "dev:convex": "convex dev",
    "dev:vite": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "lint:watch": "watchexec -e js,jsx,ts,tsx,json,css -- oxlint --type-aware",
    "serve": "vite preview",
    "start": "node --import ./dist/server/instrument.server.mjs dist/server/index.mjs",
    "test": "vitest run"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.19.0",
    "@convex-dev/react-query": "0.1.0",
    "@repo/ui": "workspace:*",
    "@sentry/tanstackstart-react": "10.32.1",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.16",
    "@tanstack/react-router": "1.144.0",
    "@tanstack/react-router-ssr-query": "1.144.0",
    "@tanstack/react-start": "1.145.5",
    "@tanstack/router-plugin": "1.145.4",
    "@workos-inc/authkit-react": "0.15.0",
    "convex": "1.31.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/devtools-vite": "0.4.0",
    "@tanstack/react-devtools": "0.9.0",
    "@tanstack/react-query-devtools": "5.91.2",
    "@tanstack/react-router-devtools": "1.144.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260105.1",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "jsdom": "27.4.0",
    "vite": "7.3.0",
    "vitest": "4.0.16",
    "web-vitals": "5.1.0",
    "wrangler": "4.56.0"
  }
}
