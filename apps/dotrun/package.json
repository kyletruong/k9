{
  "name": "dotrun",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && cp instrument.server.mjs dist/server",
    "deploy": "wrangler deploy",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "dev:convex": "convex dev",
    "lint": "oxlint --type-aware",
    "preview": "NODE_OPTIONS='--import ./instrument.server.mjs' vite preview",
    "test": "vitest run",
    "types": "tsgo --noEmit",
    "types:watch": "tsgo --noEmit --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.23.1",
    "@convex-dev/react-query": "0.1.0",
    "@repo/ui": "workspace:*",
    "@sentry/tanstackstart-react": "10.38.0",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-router": "1.159.5",
    "@tanstack/react-router-ssr-query": "1.159.5",
    "@tanstack/react-start": "1.159.5",
    "@tanstack/router-plugin": "1.159.5",
    "@workos-inc/authkit-react": "0.16.0",
    "convex": "1.31.7",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/devtools-vite": "0.5.1",
    "@tanstack/react-devtools": "0.9.5",
    "@tanstack/react-query-devtools": "5.91.3",
    "@tanstack/react-router-devtools": "1.159.5",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.2.2",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "@vitejs/plugin-react": "5.1.3",
    "babel-plugin-react-compiler": "1.0.0",
    "jsdom": "28.0.0",
    "vite": "7.3.1",
    "vite-plugin-oxlint": "1.6.0",
    "vitest": "4.0.18",
    "web-vitals": "5.1.0",
    "wrangler": "4.63.0"
  }
}
