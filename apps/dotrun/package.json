{
  "name": "dotrun",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && cp instrument.server.mjs dist/server",
    "deploy": "wrangler deploy",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "dev:convex": "convex dev",
    "format": "oxfmt --check",
    "format:watch": "watchexec -r --debounce 200ms -- oxfmt --check",
    "lint": "oxlint --type-aware",
    "lint:watch": "watchexec -e js,jsx,ts,tsx -r --debounce 200ms -- oxlint --type-aware",
    "preview": "NODE_OPTIONS='--import ./instrument.server.mjs' vite preview",
    "test": "vitest run",
    "types": "tsgo --noEmit",
    "types:watch": "tsgo --noEmit --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.20.1",
    "@convex-dev/react-query": "0.1.0",
    "@repo/ui": "workspace:*",
    "@sentry/tanstackstart-react": "10.33.0",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.16",
    "@tanstack/react-router": "1.147.3",
    "@tanstack/react-router-ssr-query": "1.147.3",
    "@tanstack/react-start": "1.149.2",
    "@tanstack/router-plugin": "1.149.0",
    "@workos-inc/authkit-react": "0.15.0",
    "convex": "1.31.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/devtools-vite": "0.4.1",
    "@tanstack/react-devtools": "0.9.2",
    "@tanstack/react-query-devtools": "5.91.2",
    "@tanstack/react-router-devtools": "1.149.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.7",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260112.1",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "jsdom": "27.4.0",
    "vite": "7.3.1",
    "vitest": "4.0.17",
    "web-vitals": "5.1.0",
    "wrangler": "4.58.0"
  }
}
