{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_date": "2025-12-17",
  "compatibility_flags": ["nodejs_compat"],
  "main": "src/server.ts",
  "name": "dotdev",
  "routes": [{ "pattern": "k9.dev", "custom_domain": true }],
  "observability": {
    "logs": {
      "enabled": true,
    },
    "traces": {
      "enabled": true,
    },
  },
  "placement": { "mode": "smart" },
  "preview_urls": true,
  "workers_dev": false,
}
