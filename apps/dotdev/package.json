{
  "name": "dotdev",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "vite dev --port 3001",
    "format": "oxfmt --check",
    "format:watch": "watchexec -r --debounce 200ms -- oxfmt --check",
    "lint": "oxlint --type-aware",
    "lint:watch": "watchexec -e js,jsx,ts,tsx -r --debounce 200ms -- oxlint --type-aware",
    "preview": "vite preview",
    "types": "tsgo --noEmit",
    "types:watch": "tsgo --noEmit --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.19.0",
    "@react-three/fiber": "9.5.0",
    "@repo/ui": "workspace:*",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-router": "1.145.7",
    "@tanstack/react-start": "1.145.8",
    "@tanstack/router-plugin": "1.145.7",
    "katex": "0.16.27",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "4.1.18",
    "three": "0.182.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@resvg/resvg-js": "2.6.2",
    "@tanstack/devtools-vite": "0.4.1",
    "@tanstack/react-devtools": "0.9.1",
    "@tanstack/react-router-devtools": "1.145.7",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/three": "0.182.0",
    "@typescript/native-preview": "7.0.0-dev.20260106.1",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "satori": "0.18.3",
    "vite": "7.3.0",
    "wrangler": "4.56.0"
  }
