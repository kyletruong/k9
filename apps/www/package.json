{
  "name": "www",
  "private": true,
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint:watch": "watchexec -e js,jsx,ts,tsx,json,css -- biome check",
    "dev:vite": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "dev:convex": "convex dev",
    "build": "vite build && cp instrument.server.mjs dist/server",
    "serve": "vite preview",
    "test": "vitest run",
    "deploy": "wrangler deploy",
    "start": "node --import ./dist/server/instrument.server.mjs dist/server/index.mjs"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.14.0",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@sentry/tanstackstart-react": "10.24.0",
    "@tailwindcss/vite": "4.1.17",
    "@tanstack/react-devtools": "0.7.11",
    "@tanstack/react-query": "5.90.7",
    "@tanstack/react-query-devtools": "5.90.2",
    "@tanstack/react-router": "1.135.2",
    "@tanstack/react-router-devtools": "1.135.2",
    "@tanstack/react-router-ssr-query": "1.135.2",
    "@tanstack/react-start": "1.135.2",
    "@tanstack/router-plugin": "1.135.2",
    "@workos-inc/authkit-react": "0.13.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "convex": "1.29.0",
    "lucide-react": "0.544.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.4.0",
    "vite-tsconfig-paths": "5.1.4",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/devtools-vite": "0.3.11",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.19.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "babel-plugin-react-compiler": "1.0.0",
    "jsdom": "27.1.0",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vitest": "3.2.4",
    "web-vitals": "5.1.0",
    "wrangler": "4.46.0"
  }
}
