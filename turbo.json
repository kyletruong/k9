{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "//#format": {},
    "//#lint": {},
    "build": {
      "env": ["VITE_*"],
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {},
    "format:watch": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:watch": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "types": {},
    "types:watch": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "tui"
}
