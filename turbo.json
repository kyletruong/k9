{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "//#format": {},
    "//#lint": {},
    "build": {
      "dependsOn": ["^build"],
      "env": ["VITE_*"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "deploy:prod": {
      "dependsOn": ["build"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "with": ["types:watch", "lint:watch", "format:watch"]
    },
    "format": {},
    "format:watch": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["transit"]
    },
    "lint:watch": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["transit"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "types": {
      "dependsOn": ["transit"]
    },
    "types:watch": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "tui"
}
