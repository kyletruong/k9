{
  "name": "k9",
  "private": true,
  "scripts": {
    "check": "turbo run //#lint:repo types //#format:repo --continue --log-order=grouped",
    "format:repo": "oxfmt --check",
    "format:repo:fix": "oxfmt",
    "lint:repo": "oxlint --type-aware",
    "lint:repo:fix": "oxlint --type-aware --fix-dangerously",
    "types:repo": "turbo run types",
    "types:repo:gen": "turbo run types:gen",
    "deploy:dotdev": "pnpm --filter=dotdev run deploy",
    "dev:dotdev": "turbo run dev --filter=dotdev",
    "dev:dotrun": "turbo run dev --filter=dotrun",
    "dev:sandbox": "turbo run dev --filter=sandbox"
  },
  "devDependencies": {
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0",
    "oxlint-tsgolint": "0.11.0",
    "turbo": "2.7.4"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.28.0"
}
