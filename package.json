{
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "turbo": "2.6.3"
  },
  "engines": {
    "node": ">=24"
  },
  "name": "k9",
  "packageManager": "pnpm@10.24.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev:www": "turbo run dev:vite dev:convex lint:watch check-types:watch --filter=www",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:fix-unsafe": "biome check --write --unsafe",
    "lint:staged": "biome check staged"
  }
}
