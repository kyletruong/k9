{
  "name": "k9",
  "private": true,
  "scripts": {
    "check": "turbo run //#lint types //#format --continue",
    "format": "oxfmt --check",
    "format:fix": "oxfmt",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix-dangerously",
    "types": "turbo run types",
    "dotdev#deploy": "pnpm --filter=dotdev run deploy",
    "dotdev#dev": "turbo run dev --filter=dotdev",
    "dotrun#dev": "turbo run dev --filter=dotrun",
    "sandbox#dev": "turbo run dev --filter=sandbox"
  },
  "devDependencies": {
    "oxfmt": "0.23.0",
    "oxlint": "1.38.0",
    "oxlint-tsgolint": "0.11.0",
    "turbo": "2.7.3"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.28.0"
}
